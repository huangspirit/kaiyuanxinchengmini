function bankType(val) {
  if (val == 'ICBC') {
    return '工商银行'
  }
  if (val == 'ABC') {
    return '农业银行'
  }
  if (val == 'CCB') {
    return '建设银行'
  }
  if (val == 'CMB') {
    return '招商银行'
  }
  if (val == 'COMM') {
    return '交通银行'
  }
  if (val == 'alipay') {
    return '支付宝'
  }
}
module.exports = {
  formatDate: formatDate,
  priveLevel: priveLevel,
  bankType: bankType
}