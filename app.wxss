/**app.wxss**/
@import "colorui/main.wxss";
@import "colorui/icon.wxss";
@import "wxParse/wxParse.wxss";

page {
  min-height: 100%;
  box-sizing: border-box;
}

.prompt {
  width: 100%;
  text-align: center;
  line-height: 90rpx;
}

view, text {
  font-family: PingFangSC-Regular;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
