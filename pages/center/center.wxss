/* pages/center/center.wxss */
.nav0.seller{
  background:linear-gradient(90deg,rgba(23,57,146,1) 0%,rgba(8,65,146,1) 100%);
}
.seller .nav-title .center{
color:#fff;
}
.footer{
  display: flex;
  position: fixed;
  bottom: 0;
  left:0;
  width:100%;
  height:90rpx;
  line-height: 90rpx;
  background: #fff;
  border-top:1rpx solid #dedede;
}
.footer>view{
  flex:1;
  text-align: center;
}
.footer .one.active{
  background:linear-gradient(90deg,rgba(255,208,0,1) 0%,rgba(255,189,0,1) 100%);
  font-size: 30rpx;
}
.footer .two.active{
  background:linear-gradient(90deg,rgba(23,57,146,1) 0%,rgba(8,65,146,1) 100%);
  font-size: 30rpx;
  color:#fff;
}