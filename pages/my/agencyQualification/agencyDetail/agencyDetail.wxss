/* pages/my/agencyQualification/agencyDetail/agencyDetail.wxss */

.userInfoList {
  width: 100%;
}

.userInfoList>view {
  display: flex;
  width: 100%;
  padding: 0 20rpx;
  height: 99rpx;
  line-height: 99rpx;
  color: #333;
  font-size: 28rpx;
  background: #fff;
  margin-bottom: 2rpx;
  justify-content: space-between;
}

.userInfoList>view>image {
  width: 15rpx;
  height: 26rpx;
  margin-top: 35rpx;
}

.userInfoList>view>view>text {
  margin-right: 15rpx;
  color: #bbb;
}

.userInfoList>view>view>image {
  width: 15rpx;
  height: 26rpx;
  vertical-align: middle;
  margin-top: -5rpx;
}

.userInfoFooter {
  padding: 20rpx;
}

.userInfoFooter>.loginOut {
  display: block;
  width: 100%;
  height: 90rpx;
  background: #df3f2f;
  text-align: center;
  line-height: 90rpx;
  color: #fff;
  margin-bottom: 20rpx;
}

.userInfoFooter>.addAgency {
  display: block;
  width: 100%;
  height: 90rpx;
  background: #bbb;
  text-align: center;
  line-height: 90rpx;
  color: #fff;
}

.agencyPic {
  position: fixed;
  width: 100%;
  height: 100%;
  background: #fff;
  top: 0;
  left: 0;
  display: flex;
  flex-direction: column;
  padding: 20rpx;
  text-align: center;
}

.agencyPic>text {
  display: block;
  text-align: right;
  line-height: 80rpx;
}

.agencyPic>image {
  margin: auto;
  max-height: 100%;
  max-width: 100%;
}
