/* pages/my/agencyQualification/agencyQualification.wxss */

.agencyQualification {
  width: 100%;
}

.agencyQualification>.listContent {
  padding: 20rpx;
  background: #fff;
}

.agencyQualification>.listContent .listTop {
  display: flex;
  justify-content: space-between;
  margin-bottom: 15rpx;
}

.agencyQualification>.listContent .listTop>text:nth-of-type(1) {
  color: #333;
  font-weight: bold;
  font-size: 35rpx;
}

.agencyQualification>.listContent .listTop>.end {
  color: #3fd666;
}

.agencyQualification>.listContent .listTop>.wait {
  color: #d5a260;
}

.agencyQualification>.listContent .listTop>.fail {
  color: #df3f2f;
}

.agencyQualification>.listContent .listBottom {
  display: flex;
  flex-direction: column;
  color: #999;
}

.agencyQualification>.listContent .listBottom>text {
  line-height: 40rpx;
}

.agencyQualification>.footerBtn {
  padding: 20rpx;
}

.agencyQualification>.footerBtn>text {
  display: block;
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  background: #df3f2f;
  color: #fff;
  border-radius: 8rpx;
}

.agencyQualification>.footerBtn>text:active {
  background: rgba(223, 63, 47, 0.8);
}
