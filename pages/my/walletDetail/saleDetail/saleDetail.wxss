/* pages/my/walletDetail/saleDetail/saleDetail.wxss */

.bankList {
  width: 100%;
  height: 140rpx;
  padding: 40rpx 20rpx;
  background: #fff;
  display: flex;
  justify-content: space-between;
  margin:10rpx 0;
}
.bankList>view:nth-of-type(1) {
  display: flex;
  flex-direction: column;
}
.bankList>view:nth-of-type(1)>text:nth-of-type(1) {
  font-size: 30rpx;
  font-weight: bold;
  color: #333;
  margin-bottom: 10rpx;
}
.bankList>view:nth-of-type(1)>text:nth-of-type(2) {
  font-size: 20rpx;
  color: #999999;
}
.bankList>view:nth-of-type(2)>text {
  font-size: 40rpx;
  font-weight: bold;
  color: #333;
}
.bankNo {
  width: 100%;
  height: 400rpx;
  line-height: 400rpx;
  text-align: center;
  padding: 0 50rpx;
}
