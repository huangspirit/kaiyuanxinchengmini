.shopingCarEmpty {
  width: 100%;
  text-align: center;
  padding: 135rpx 0;
}

.shopingCarEmpty>image {
  width: 295rpx;
  height: 255rpx;
}

.shopingCarEmpty>view {
  display: block;
  font-size: 30rpx;
  color: #333;
  margin-top: 75rpx;
}
.shopingCarEmpty>view>text {
  color: blue;
}
.shopingCarEmpty>view>text:active {
  color: rgba(0,0,255,.8)
}
.shopingCarEmpty .wantTitle {
  position: relative;
}

.shopingCarEmpty .wantTitle>image {
  width: 100%;
  height: 18rpx;
  padding: 0 20rpx;
}

.shopingCarEmpty .wantTitle>text {
  font-size: 18rpx;
  color: #999;
  position: absolute;
  z-index: 1;
  left: 44%;
  top: 10rpx;
}

.shopingCarEmpty .wantList {
  width: 100%;
  padding: 20rpx;
  background: #fff;
}

.shopingCarEmpty .wantList .listDetail {
  display: flex;
  margin-top: 20rpx;
}

.shopingCarEmpty .wantList .listDetail .listLeft {
  width: 148rpx;
  height: 148rpx;
  border: 2rpx solid #e5e5e5;
  margin: 0 45rpx 0 20rpx;
}

.shopingCarEmpty .wantList .listDetail .listLeft>image {
  width: 100%;
  height: 100%;
}

.shopingCarEmpty .wantList .listDetail .listRight {
  border-bottom: 2rpx solid #ddd;
  width: calc(100% - 148rpx);
  text-align: left;
  padding-bottom: 25rpx;
}

.shopingCarEmpty .wantList .listDetail .listRight>text {
  display: block;
}

.shopingCarEmpty .wantList .listDetail .listRight>text:nth-of-type(1) {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #333;
  font-weight: bold;
  font-size: 24rpx;
}

.shopingCarEmpty .wantList .listDetail .listRight>text:nth-of-type(2) {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #999;
  margin: 8rpx 0;
  font-size: 24rpx;
}

.shopingCarEmpty .wantList .listDetail .listRight>text:nth-of-type(3) {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  color: #333;
  font-size: 18rpx;
  margin-bottom: 10rpx;
}

.shopingCarEmpty .wantList .listDetail .listRight .goodsPrice>text {
  font-weight: bold;
  color: #df3f2f;
  line-height: 40rpx;
}
