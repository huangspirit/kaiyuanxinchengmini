.shoppingCart .cartTop {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  padding: 0 20rpx;
  margin-bottom:20rpx;
  display: flex;
  justify-content: space-between;
}

.shoppingCart .cartTop>view {
  width: 400rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: flex;
}

.shoppingCart .cartTop>view>image {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-right: 10rpx;
  margin-top: 22rpx;
}

.shoppingCart .cartTop>view>picker {
  font-size: 30rpx;
  color: #999;
}

.shoppingCart .cartTop>text {
  float: right;
  font-size: 30rpx;
  color: #999;
}

